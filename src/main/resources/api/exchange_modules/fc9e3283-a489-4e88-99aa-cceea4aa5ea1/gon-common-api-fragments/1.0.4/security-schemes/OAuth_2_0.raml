#%RAML 1.0 SecurityScheme
type: OAuth 2.0
description: This API supports OAuth 2.0 Authorization
displayName: oauth_2_0
describedBy:
  headers:
    Authorization: 
      description: Used to send a valid OAuth 2.0 Bearer Token.
      type: string
settings:
  authorizationUri: http://0.0.0.0:8081/authorize
  accessTokenUri: http://0.0.0.0:8081/access-token
  authorizationGrants: [client_credentials]